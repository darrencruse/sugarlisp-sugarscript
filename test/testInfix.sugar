console.log('NOW TESTS FOCUSING ON PRECEDENCE...');

// simple (equal precedence)
console.log('EQUAL PRECEDENCE');
console.log(3 - 4 + 5);
console.log(3 + 4 - 5);
// times before plus:
console.log('UNEQUAL PRECEDENCE');
console.log(3 + 4 * 5);
console.log(3 * 4 + 5);
// a little more complex:
console.log(3 * 4 + 5 / 6);
console.log(3 / 4 + 5 * 6);
console.log(3 * 4 / 5 + 6 - 7);
console.log(3 - 4 / 5 * 6 + 7);

// dot before anything
console.log('DOT PRECEDENCE');
